{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Note",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "title": { "type": "string" },
    "content": { "type": "string" },
    "createdAt": { "type": "string", "format": "date-time" },
    "updatedAt": { "type": "string", "format": "date-time" }
  },
  "required": ["id", "title", "content", "createdAt", "updatedAt"]
}
